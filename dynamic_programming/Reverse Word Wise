#include <iostream>
#include "solution.h"
#include<cstring>
using namespace std;
void reverseStringWordWise(char input[]) { 
    int length=strlen(input);
    
        
        int i=0,j=length-1;
        while(i<j){
            char temp=input[i];
            input[i]=input[j];
            input[j]=temp;
            i++;
            j--;
        }
    
    int k,z=0;
    for(k=0;input[k]!='\0';k++)
    {
        if(input[k]==' ' || k==length-1)
        {
          int w=k-1;
            if(k==length-1)
            {
                w=length-1;
            }
          while(z<w)
          {
              char temp1=input[z];
            input[z]=input[w];
            input[w]=temp1;
              z++;
              w--;
          }
            z=k+1;
            
        }
    }

int main() {
    char input[1000];
    cin.getline(input, 1000);
    reverseStringWordWise(input);
    cout << input << endl;
}
